	<PG name="buttonQuestion">		<SCR name="getPageID"><![CDATA[// SET UP PAGE & GET INFO NEEDED FROM XML// recalculate variable valuesvaluesCalc = 0;getAlgebraInfo();pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();myInfo = pageIcon.templateData.buttonQuestion[0];rootIcon.setTitleText(XMLdata.name);align = myInfo.align;panelWidth = myInfo.panelWidth;if (panelWidth == "Small"){  panelW = 300;  textW = 440;} else if (panelWidth == "Medium"){  panelW = 450;  textW = 290;} else if (panelWidth == "Large"){  panelW = 600;  textW = 150;} else {  panelW = 760;  textW = 0;}if (myInfo.narration != undefined){  narration = myInfo.narration;  rootIcon.broadcast('showAudio');}]]></SCR>		<DIS w="panelW" h="474" tabIndex="-1" id="panel" name="panel" x="align == 'Right' ? 20 : 780 - panelW" y="20" type="ext" url="templatePath + 'common/whitePanel.swf'"><![CDATA[]]></DIS>		<DIS id="instruction" embedVars="1" name="instruction" x="align == 'Right' ? 40 + panelW : 20" y="panel._y" w="740 - panelW" h="10" type="text"><![CDATA[{myInfo.instruction}]]></DIS>		<DIS embedVars="1" textColour="#333333" id="prpt" name="question" x="panel._x + 20" y="panel._y + 20" w="panelW - 40" h="10" type="text"><![CDATA[{myInfo.prompt}]]></DIS>		<INT id="buttonInteraction" name="buttonInteraction" perpetual="0">			<RES rs="btn" swf="templatePath + 'common/button.swf'" name="button" type="button" x="prpt._x" y="prpt._y + prpt._height + 20" label="{myInfo.label}" erase="1" exit="0">				<DIS id="feedbackTxt" embedVars="1" scrolling="auto" textColour="#333333" name="feedback" x="btn._x" y="btn._y + 40" w="panelW - 40" h="474 - (btn._y + btn._height) - 10" type="text"><![CDATA[{myInfo.feedback}]]></DIS>				<SCR name="btnScript"><![CDATA[restartBtn._visible = true;btn._visible = false;]]></SCR>			</RES>			<RES visible="0" rs="restartBtn" swf="templatePath + 'common/button.swf'" name="restartBtn" type="button" x="btn._x" y="btn._y" label="Restart" erase="1" exit="0">				<SCR name="restart"><![CDATA[rootIcon.gotoPage(rootIcon.getPageIndex() - 1);]]></SCR>			</RES>		</INT>	</PG>